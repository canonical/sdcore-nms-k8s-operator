configuration:
  managedByConfigPod:
    enabled: true
    syncUrl: ""
  mongodb:
    name: {{ common_database_name }}
    url: {{ common_database_url }}
    authKeysDbName: {{ auth_database_name }}
    authUrl: {{ auth_database_url }}
    webuiDbName: {{ webui_database_name }}
    webuiDbUrl: {{ webui_database_url }}
  spec-compliant-sdf: false
  enableAuthentication: true
  ui_tls:
    key: {{ ui_tls_key_path }}
    pem: {{ ui_tls_certificate_path }}
  config_tls:
    key: {{ config_tls_key_path }}
    pem: {{ config_tls_certificate_path }}
  send_pebble_notifications: true
info:
  description: WebUI initial local configuration
  version: 1.0.0
  http-version: 2
logger:
  WEBUI:
    debugLevel: {{ log_level }}
